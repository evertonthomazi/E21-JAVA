{
  "name": "braspag/braspag-php-sdk",
  "description": "SDK para integração com plataforma Braspag",
  "type": "library",
  "license" : "MIT",
  "authors": [
    {
      "name": "Ricardo Abdalla",
      "email": "rabdalla@braspag.com.br"
    },
    {
      "name": "Maurici Ferreira Junior",
      "email": "mjunior@braspag.com.br"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.37",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "BraspagSdk\\": "src/BraspagSdk"
    }
  },
  "autoload-dev": {
    "psr-4": { "BraspagSdk\\Tests\\": "tests/" }
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  }
}